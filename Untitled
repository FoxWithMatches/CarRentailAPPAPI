+//
+//  Car.swift
+//  CarRentailAPP
+//
+//  Created by Alisa Ts on 03.12.2021.
+//
+
+struct Car: Decodable {
+    let name: String?
+    let avg_horsepower: Double?
+    let avg_price: Double?
+    let img_url: String?
+}
